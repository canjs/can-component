{
  "name": "can-component",
  "version": "5.0.0",
  "description": "Custom elements for CanJS",
  "homepage": "https://canjs.com/doc/can-component.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/canjs/can-component.git"
  },
  "author": {
    "name": "Bitovi",
    "email": "contact@bitovi.com",
    "url": "http://bitovi.com"
  },
  "scripts": {
    "preversion": "npm test",
    "postpublish": "git push --tags && git push",
    "testee": "testee test/test.html --browsers firefox",
    "test": "npm run detect-cycle && npm run jshint && npm run testee",
    "jshint": "jshint ./*.js test/*.js --config",
    "release:pre": "npm version prerelease && npm publish --tag=pre",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "develop": "done-serve --static --develop --port 8080",
    "detect-cycle": "detect-cyclic-packages --ignore done-serve",
    "docco": "docco -o docco can-component.js",
    "docco:publish": "npm run docco && gh-pages -d docco"
  },
  "main": "can-component",
  "keywords": [
    "canjs",
    "canjs-plugin",
    "donejs"
  ],
  "system": {
    "configDependencies": [
      "live-reload"
    ]
  },
  "dependencies": {
    "can-assign": "^1.1.1",
    "can-bind": "^1.5.0",
    "can-child-nodes": "^1.0.0",
    "can-construct": "^3.2.0",
    "can-control": "^5.0.0",
    "can-define": "^2.0.0",
    "can-dom-data": "^1.0.1",
    "can-dom-events": "^1.1.0",
    "can-dom-mutate": "^2.0.0",
    "can-fragment": "^1.0.0",
    "can-globals": "^1.0.0",
    "can-log": "^1.0.0",
    "can-namespace": "1.0.0",
    "can-observation": "^4.0.0",
    "can-observation-recorder": "^1.2.0",
    "can-queues": "^1.3.0",
    "can-reflect": "^1.6.0",
    "can-simple-map": "^4.1.0",
    "can-simple-observable": "^2.0.0",
    "can-stache": "^5.0.0",
    "can-stache-bindings": "^5.0.0",
    "can-stache-key": "^1.0.0",
    "can-string": "<2.0.0",
    "can-symbol": "^1.4.1",
    "can-view-callbacks": "^5.0.0",
    "can-view-model": "^4.0.0",
    "can-view-scope": "^4.7.0"
  },
  "devDependencies": {
    "can-diff": "^1.5.0",
    "can-observe": "^2.0.0",
    "can-test-helpers": "^1.1.2",
    "can-value": "<2.0.0",
    "can-vdom": "^4.0.0",
    "can-simple-dom": "^1.7.0",
    "detect-cyclic-packages": "^1.1.0",
    "docco": "^0.8.0",
    "done-serve": "^2.0.0",
    "gh-pages": "^2.0.1",
    "highlight.js": "^9.15.7",
    "jshint": "^2.9.1",
    "steal": "^1.0.0",
    "steal-qunit": "^2.0.0",
    "steal-tools": "^1.0.1",
    "testee": "^0.9.0"
  }
}
